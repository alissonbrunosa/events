<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
  	<div class="col-xs-4 col-sm-2">
  		<%= link_to(omniauth_authorize_path(resource_name, provider), class: "btn btn-lg btn-block omb_btn-#{provider}") do %>
        <i class="fa fa-<%= provider %> visible-xs"></i>
        <span class="hidden-xs"><%= t provider %></span>
      <% end -%>
    </div>
  <% end -%>
<% end -%>
